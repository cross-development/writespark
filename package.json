{
	"name": "writespark",
	"version": "1.0.0",
	"description": "Micro blog application",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"start": "node ./build/src/main.js",
		"dev": "nodemon",
		"build": "tsc",
		"lint": "eslint ./src/**",
		"lint:fix": "eslint ./src/** --fix",
		"debug": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --name",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:seed": "ts-node --transpile-only ./src/persistence/seed.ts"
	},
	"prisma": {
		"schema": "./src/persistence/schema.prisma"
	},
	"keywords": [],
	"author": "Vitalii Derda",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^5.11.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"colors": "^1.4.0",
		"config": "^3.3.11",
		"connect-flash": "^0.1.1",
		"cookie-parser": "^1.4.6",
		"date-fns": "^3.6.0",
		"dotenv": "^16.4.5",
		"express": "^4.18.3",
		"express-handlebars": "^7.1.2",
		"helmet": "^7.1.0",
		"inversify": "^6.0.2",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"pg": "^8.11.3",
		"prisma": "^5.11.0",
		"reflect-metadata": "^0.2.1"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/config": "^3.3.3",
		"@types/connect-flash": "^0.0.40",
		"@types/cookie-parser": "^1.4.7",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.28",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.2"
	}
}
